{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug current CmakeTarget",
            "cwd": "${workspaceFolder}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "openocd",
            "device": "STM32F446ZE",
            "configFiles": [
                "modm/openocd.cfg",
            ],
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 180000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "Print",
                        "port": 0,
                    },
                ]
            }
        }
    ]
}